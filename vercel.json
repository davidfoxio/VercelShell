{
  "name": "how-now",
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": [
          "api/**"
        ]
      }
    }
  ],
  "headers": [
    {
    "source": "/_nuxt/.+",
      "headers": {
        "cache-control": "s-maxage=31536000"
      }
    }
  ],
  "redirects": [

  ],
  "routes": [
    {
      "src": "/_nuxt/.+",
      "headers": {
        "cache-control": "s-maxage=31536000"
      }
    },
    { "source": "/dazn-2018(\/*)$", "destination": "/work" },
    { "source": "/creative(\/*)$", "destination": "/contact" },
    { "source": "/dancing-on-ice(\/*)$", "destination": "/work" },
    { "source": "/dazn(\/*)$", "destination": "/work/dazn-global-rebranding" },
    { "source": "/ffrees-the-unbaking-revolution(\/*)$", "destination": "/work/ffrees" },
    { "source": "/got-to-dance(\/*)$", "destination": "/work" },
    { "source": "/heart-bingo(\/*)$", "destination": "/work/heart-bingo" },
    { "source": "/heart-brand-ad(\/*)$", "destination": "/work/heart-brand-campaign" },
    { "source": "/heart-happy-summer-days(\/*)$", "destination": "/work/hearts-happy-summer-days" },
    { "source": "/heart-sponsors-friends(\/*)$", "destination": "/work/heart-sponsors-friends" },
    { "source": "/itv-news-rebrand-film(\/*)$", "destination": "/work/itv-news" },
    { "source": "/itv-player-on-android(\/*)$", "destination": "/work/itv-idents" },
    { "source": "/itv-player-on-second-screen(\/*)$", "destination": "/work/itv-idents" },
    { "source": "/itv-rebrand-idents(\/*)$", "destination": "/work/itv-idents" },
    { "source": "/jobs(\/*)$", "destination": "/contact" },
    { "source": "/junior-motion-graphic-designer(\/*)$", "destination": "/contact" },
    { "source": "/keep-it-in-ground(\/*)$", "destination": "/work/the-guardian" },
    { "source": "/lbc-campaign-2018(\/*)$", "destination": "/work/lbc-campaigns" },
    { "source": "/motion-graphic-designer(\/*)$", "destination": "/contact" },
    { "source": "/new-gallery(\/*)$", "destination": "/work/itv-idents" },
    { "source": "/new-gallery-1(\/*)$", "destination": "/work/itv-idents" },
    { "source": "/nfl-gamepass-perform(\/*)$", "destination": "/work/nfl-gamepass" },
    { "source": "/samsung-the-next-galaxy(\/*)$", "destination": "/work/samsung-the-next-galaxy" },
    { "source": "/singing-in-the-rainforest(\/*)$", "destination": "/work" },
    { "source": "/technical-assistant(\/*)$", "destination": "/contact" },
    { "source": "/times-new-roman(\/*)$", "destination": "/work/the-times" },
    { "source": "/towie(\/*)$", "destination": "/work/towie" },
    { "source": "/nfl-game-pass-2018(\/*)$", "destination": "/work/nfl-gamepass" },
    { "source": "/heart-brand-campaign-2018-global-roadio(\/*)$", "destination": "/work/heart-brand-campaign" },
    { "source": "/lbc-on-tv-global-radio(\/*)$", "destination": "/work/lbc-campaigns" },
    { "source": "/lbc-uselection2016(\/*)$", "destination": "/work/lbc-campaigns" },
    { "source": "/dazn-2018(\/*)$", "destination": "/work/dazn-global-rebranding" },
    { "source": "/dazn-mvm(\/*)$", "destination": "/work/dazn-global-rebranding" },
    { "source": "/welcome(\/*)$", "destination": "/" },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
